// This is your Prisma schema file,

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String   @id @unique @default(uuid()) 
  email String @unique
  password String 
  biometricKey String? @default("")
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())

}
model Challenge{
  id String @id @unique @default(uuid())
  challenge String

}





